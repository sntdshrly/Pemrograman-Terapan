<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="350.0" prefWidth="383.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo_kelas_fx_coll.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <ComboBox fx:id="cmb1" prefWidth="150.0" />
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <ListView fx:id="listview1" prefHeight="200.0" prefWidth="200.0" />
         <TableView fx:id="table1" prefHeight="200.0" prefWidth="200.0">
           <columns>
             <TableColumn fx:id="column1" prefWidth="75.0" text="C1" />
             <TableColumn fx:id="column2" prefWidth="75.0" text="C2" />
           </columns>
         </TableView>
      </children>
   </HBox>
    <Button onAction="#onHelloButtonClick" text="Hello!" />
</VBox>
